<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rect_header_profile"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_56sdp">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:background="@drawable/circal_weight"
                android:layout_centerVertical="true"
                android:padding="@dimen/_4sdp"
                android:layout_marginStart="@dimen/_8sdp"
                android:src="@drawable/ic_back_icon" />

            <TextView
                style="@style/title_basicreg_18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="@color/white"
                android:text="@string/target" />
        </RelativeLayout>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="@dimen/_12sdp"
            app:cardElevation="@dimen/_4sdp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_24sdp">

                <LinearLayout
                    android:id="@+id/mr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/name_lay"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_6sdp"
                        android:elevation="@dimen/_8sdp"
                        android:background="@drawable/roal_slect_card"
                        android:orientation="vertical">


                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_margin="@dimen/_10sdp"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="@dimen/_8sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/tru_hub_menu_bg"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:id="@+id/owner"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:padding="@dimen/_3sdp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/bai_jamjuree_medium"
                                            android:layout_gravity="center"
                                            android:text="@string/add_load"
                                            android:textColor="@color/blue"
                                            android:textSize="@dimen/_15ssp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="@dimen/_16sdp"
                                            android:fontFamily="@font/bai_jamjuree_medium"
                                            android:text="30"
                                            android:layout_gravity="center"
                                            android:textColor="@color/blue"
                                            android:textSize="@dimen/_16ssp" />


                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>
                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_margin="@dimen/_10sdp"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="@dimen/_8sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/tru_hub_menu_bg"
                                android:orientation="vertical">

                                <TextView
                                    style="@style/label_semi"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:text="@string/truckSupplier"
                                    />

                                <LinearLayout
                                    android:id="@+id/broker"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            style="@style/regular_text_12"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:text="@string/truckSupplier"
                                            android:textColor="@color/primery_text"
                                            />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="@dimen/_5sdp"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/bai_jamjuree_medium"
                                            android:text="30"
                                            android:textColor="@color/blue"
                                            android:textSize="@dimen/_16ssp" />


                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:text="@string/broker_new"
                                            android:singleLine="true"
                                            android:textColor="@color/primery_text"
                                            style="@style/regular_text_12"/>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="@dimen/_5sdp"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/bai_jamjuree_medium"
                                            android:text="30"
                                            android:textColor="@color/blue"
                                            android:textSize="@dimen/_16ssp" />


                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            style="@style/regular_text_12"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:text="@string/growthpartner"
                                            android:textColor="@color/primery_text"
                                            />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="@dimen/_5sdp"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/bai_jamjuree_medium"
                                            android:text="30"
                                            android:textColor="@color/blue"
                                            android:textSize="@dimen/_16ssp" />


                                    </LinearLayout>

                                </LinearLayout>


                            </LinearLayout>
                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_margin="@dimen/_10sdp"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="@dimen/_8sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/tru_hub_menu_bg"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/tru_hub_menu_bg"
                                    android:orientation="vertical">

                                    <TextView
                                        style="@style/label_semi"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="@string/vas"
                                        />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                style="@style/regular_text_12"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/vehicle_tracking"
                                                android:textColor="@color/primery_text"
                                                />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/vehicle_verify"
                                                android:singleLine="true"
                                                android:textColor="@color/primery_text"
                                                style="@style/regular_text_12"/>

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                style="@style/regular_text_12"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/verify_dl"
                                                android:textColor="@color/primery_text"
                                                />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                style="@style/regular_text_12"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/insurance"
                                                android:textColor="@color/primery_text"
                                                />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/finance"
                                                android:singleLine="true"
                                                android:textColor="@color/primery_text"
                                                style="@style/regular_text_12"/>

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:orientation="vertical">

                                            <TextView
                                                style="@style/regular_text_12"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:text="@string/smart_fuel"
                                                android:textColor="@color/primery_text"
                                                />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginBottom="@dimen/_5sdp"
                                                android:layout_gravity="center"
                                                android:fontFamily="@font/bai_jamjuree_medium"
                                                android:text="30"
                                                android:textColor="@color/blue"
                                                android:textSize="@dimen/_16ssp" />


                                        </LinearLayout>

                                    </LinearLayout>
                                </LinearLayout>


                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </com.google.android.material.card.MaterialCardView>

    </LinearLayout>

</LinearLayout>